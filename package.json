{
  "name": "mindstream",
  "version": "1.0.0",
  "private": true,
  "description": "Interactive web app transforming live webcam feed into AI-generated visual narrative using Livepeer Daydream",
  "scripts": {
    "dev": "pnpm --parallel --stream run dev",
    "dev:frontend": "pnpm --filter frontend dev",
    "dev:backend": "pnpm --filter backend dev",
    "build": "pnpm --stream run build",
    "build:frontend": "pnpm --filter frontend build",
    "build:backend": "pnpm --filter backend build",
    "install:all": "pnpm install"
  },
  "keywords": [
    "livepeer",
    "envio",
    "ai-video",
    "blockchain",
    "base-sepolia"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
